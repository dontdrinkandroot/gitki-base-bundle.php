<id>{{ serie.id }}</id>
<slug>{{ serie.slug }}</slug>
<name>{{ serie.name }}</name>
{% if serie.orig_name is defined %}<orig-name>{{ serie.orig_name }}</orig-name>{% endif %}
{% if serie.authors is defined %}
	{% for person in serie.authors %}
		<author>
			{% include 'App:Person:person.xml.twig' %}
		</author>
	{% endfor %}
{% endif %}
